<?xml version="1.0" encoding="UTF-8"?>
<sml:PhysicalSystem xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="top" xsi:schemaLocation="http://www.opengis.net/sensorml/2.0 http://schemas.opengis.net/sensorML/2.0/sensorML.xsd">
    
     {{#custom}} {{#contactMetaData}}
       <gml:metaDataProperty>
        <gml:GenericMetaData>
            <gmd:CI_ResponsibleParty>
           {{#if name}}
				<gmd:individualName>
                    <gco:CharacterString>{{name}}</gco:CharacterString>
                </gmd:individualName>
                 {{/if}}
                <gmd:organisationName>
                    <gco:CharacterString>YELLOW PAGES - EMSO</gco:CharacterString>
                </gmd:organisationName>
                <gmd:contactInfo>
                    <gmd:CI_Contact>
                    {{#if email}}
                        <gmd:address>
                            <gmd:CI_Address>
                                <gmd:electronicMailAddress>
<gco:CharacterString>{{email}}</gco:CharacterString>
                                </gmd:electronicMailAddress>
                            </gmd:CI_Address>
                        </gmd:address>
                        {{/if}}
                        <gmd:onlineResource>
                            <gmd:CI_OnlineResource>
                                <gmd:linkage>
<gmd:URL>http://www.esonetyellowpages.com/</gmd:URL>
                                </gmd:linkage>
                            </gmd:CI_OnlineResource>
                        </gmd:onlineResource>
                    </gmd:CI_Contact>
                </gmd:contactInfo>
                <gmd:role>
                    <gmd:CI_RoleCode codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI_RoleCode" codeListValue="pointOfContact"/>
                </gmd:role>
            </gmd:CI_ResponsibleParty>
        </gml:GenericMetaData>
    </gml:metaDataProperty>
      {{/contactMetaData}} {{/custom}}  
    <!-- Description --> 
    <gml:description> {{description}}   </gml:description>
    <!-- Name --> 
    <gml:name > {{#attrs}} {{#text}} {{name}} {{/text}} {{/attrs}} </gml:name>  
  <!-- keywords--> 
    <sml:keywords>
        <sml:KeywordList>
            <sml:keyword>marine</sml:keyword>
        </sml:KeywordList>
    </sml:keywords>

    
    <!-- Identification--> 
   
   <sml:identification>
        <sml:IdentifierList>
            <sml:identifier>
                <sml:Term definition="http://www.ifremer.fr/tematres/vocab/index.php?tema=66">
                    <sml:label>uuid</sml:label>
                    <sml:value>{{uuidModel}}</sml:value>
                </sml:Term>
            </sml:identifier>
            
               {{#custom}} {{#identifier}}   
            <sml:identifier>
              
                <sml:Term{{#if definition}} definition="{{definition}}"{{/if}}>
                    <sml:label>{{name}}</sml:label>
                    {{#if codespace}}<sml:codeSpace xlink:href="{{codespace}}"/>{{/if}}
                    <sml:value>{{URI}}</sml:value>
                </sml:Term>
              
            </sml:identifier>
             {{/identifier}} {{/custom}}
            
        </sml:IdentifierList>
    </sml:identification>
    
    <!-- Classification -->
       
    <sml:classification>
        <sml:ClassifierList>
      
            {{#custom}} {{#classifier}}   
            <sml:classifier>
              
                <sml:Term{{#if definition}} definition="{{definition}}"{{/if}}>
                    <sml:label>{{name}}</sml:label>
                    {{#if codespace}}<sml:codeSpace xlink:href="{{codespace}}"/>{{/if}}
                    <sml:value>{{URI}}</sml:value>
                </sml:Term>
              
            </sml:classifier>
             {{/classifier}} {{/custom}}
          
        </sml:ClassifierList>
    </sml:classification>
  {{#documentation}}
     <sml:documentation>
        <sml:DocumentList>         
            <sml:document>
                <gmd:CI_OnlineResource>
                    <gmd:linkage>
                        <gmd:URL>{{link}}</gmd:URL>
                    </gmd:linkage>
                    <gmd:name>
                        <gco:CharacterString>{{name}}</gco:CharacterString>
                    </gmd:name>                   
                    <gmd:function><gmd:CI_OnLineFunctionCode codeList="http://www.ifremer.fr/tematres/vocab/index.php?tema=99" codeListValue="quicklook"></gmd:CI_OnLineFunctionCode></gmd:function>
                </gmd:CI_OnlineResource>
            </sml:document>
        </sml:DocumentList>
    </sml:documentation>
   {{/documentation}}
     
     
     
   
    
</sml:PhysicalSystem>
